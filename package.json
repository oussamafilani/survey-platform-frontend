{
  "name": "babelmadina",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@auth0/auth0-react": "^1.10.1",
    "@craco/craco": "^6.4.3",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "commitizen": "^4.2.4",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hot-toast": "^2.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
